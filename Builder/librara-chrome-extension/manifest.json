{
  "manifest_version": 3,
  "name": "Epub Reader - Librera Reader",
  "version": "1.1",
  "description": "Book Reader: EPUB, PDF, CBZ, FB2",
  "permissions": ["tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "action": {
    "default_icon": {
      "16": "logo.png",
      "48": "logo.png",
      "128": "logo.png"
    }
  }
}
